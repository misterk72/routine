      "targets": [
        {
          "datasource": "MariaDB-Prod",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  w.start_time AS 'Date et Heure',\n  w.program AS 'Programme',\n  w.duration_minutes AS 'Duree (min)',\n  w.avg_speed_kmh AS 'Vitesse Moyenne (km/h)',\n  w.distance_km AS 'Distance parcourue (km)',\n  w.calories AS 'Calories',\n  w.calories_per_km AS 'Calories/km',\n  w.avg_heart_rate AS 'Moyenne pulsations/min',\n  w.max_heart_rate AS 'Max pulsations/min',\n  w.min_heart_rate AS 'Min pulsations/min',\n  w.sleep_heart_rate_avg AS 'FC Repos pulsations/min',\n  w.vo2_max AS 'VO2',\n  wm.weight_kg AS 'Masse (kg)',\n  wm.fat_mass_kg AS 'Masse graisseuse (kg)',\n  wm.fat_percentage AS 'Taux de graisse',\n  wm.waist_cm AS 'Tour de taille (cm)',\n  w.soundtrack AS 'Fond sonore',\n  w.notes AS 'Observations'\nFROM workouts w\nLEFT JOIN (\n  SELECT wm1.*\n  FROM weight_measurements wm1\n  JOIN (\n    SELECT user_id, DATE(measured_at) AS d, MAX(measured_at) AS max_at\n    FROM weight_measurements\n    GROUP BY user_id, DATE(measured_at)\n  ) latest ON latest.user_id = wm1.user_id\n    AND DATE(wm1.measured_at) = latest.d\n    AND wm1.measured_at = latest.max_at\n) wm ON wm.user_id = w.user_id\n  AND DATE(w.start_time) = DATE(wm.measured_at)\nWHERE $__timeFilter(w.start_time)\n  AND w.deleted = 0\n  AND (${user_id:raw} = 'all' OR w.user_id IN (${user_id:sqlstring}))\nORDER BY w.start_time DESC",
          "refId": "A"
        }
      ],
      "title": "Entrainement velo elliptique",
      "type": "table"
    }
